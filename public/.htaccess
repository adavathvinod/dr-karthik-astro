Options -MultiViews
RewriteEngine On

# Force HTTPS + correct domain
RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} !^drkarthikmanchala\.com$ [NC]
RewriteRule ^ https://drkarthikmanchala.com%{REQUEST_URI} [L,R=301]

# SPA routing
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^ index.html [L]